  "author": {
    "name": "Cheton Wu"
  },
  "license": "MIT",
  "dependencies": {
    "clean-css": "^4.1.5",
    "concat-stream": "^1.6.0",
    "css": "^2.2.1",
    "find-node-modules": "^1.0.4",
    "lodash": "^4.17.4",
    "mime": "^1.3.6",
    "through2": "2.0.x"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "coveralls": "^2.13.1",
    "del": "^3.0.0",
    "exorcist": "^0.4.0",
    "fs-extra": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-jshint": "^2.0.4",
    "gulp-util": "^3.0.8",
    "jsdom": "^9.12.0",
    "jshint": "^2.9.5",
    "require-dir": "^0.3.2",
    "run-sequence": "^2.0.0",
